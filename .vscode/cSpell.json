    "words": [
        "Ponduit",
        "NTFS",
        "Repo",
        "Hashtable",
        "googlemail",
        "Cmdlet",
        "appveyor",
        "GUID",
        "wildcards",
        "Cmdlets",
        "codecoverage",
        "notmatch",
        "Httpclient",
        "Multipart",
        "formdata",
        "callsign",
        "Veyor",
        "notlike",
        "Params",
        "testresults",
        "powershellgallery",
        "chocolatey",
        "choco"
